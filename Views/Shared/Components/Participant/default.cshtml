@model SiteData

    <li class="nav-item">
        <a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="Participate">ParticipateTEST and @ViewData["Phase"]</a>
    </li>
    @if (Model.SiteData_Phase == 1)
    {
        <li class="nav-item dropdown">
            <a class="nav-link text-white dropdown-toggle" data-toggle="dropdown" asp-area="" asp-controller="" asp-action="">Participate</a>
            <div class="dropdown-menu">
                <a class="dropdown-item" asp-area="" asp-controller="Home" asp-action="Phase1StudentPhase1Student">Student</a>
                <a class="dropdown-item" asp-area="" asp-controller="Home" asp-action="Phase1Educationalist">Educator</a>
                <a class="dropdown-item" asp-area="" asp-controller="Home" asp-action="Phase1Academic">Professional</a>
            </div>
        </li>
    }
    @if (Model.SiteData_Phase == 2)
    {
    <li class="nav-item">
        <a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="Phase2PreTest">Participate</a>
    </li>
    }
    @if (Model.SiteData_Phase == 3)
    {
    <li class="nav-item">
        <a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="Phase2PostTest">Participate</a>
    </li>
    }
    @if (Model.SiteData_Phase == 4)
    {
    <li class="nav-item">
        <a class="nav-link text-white" asp-area="" asp-controller="Home" asp-action="ParticipateClosed">Participate</a>
    </li>
    }

